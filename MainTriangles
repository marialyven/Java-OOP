package com.gmail.marialyven12;

public class Main {

	public static void main(String[] args) {
		Triangle triangleOne = new Triangle();
		triangleOne.setSideA(6);
		triangleOne.setSideB(6);
		triangleOne.setSideC(10);
		
		System.out.println(triangleOne);
		triangleOne.area();
		
		Triangle triangleTwo = new Triangle();
		triangleTwo.setSideA(10);
		triangleTwo.setSideB(13);
		triangleTwo.setSideC(17);
			
		System.out.println(triangleTwo);	
		triangleTwo.area();
	}	

}
