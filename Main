package com.gmail.marialyven12;

import java.io.File;
import java.io.IOException;


public class Main {

	public static void main(String[] args) {
		MyFileFilter mFF = new MyFileFilter("pdf");
		File folder = new File("Users/marialyven/Downloads/76_Sample_Chapter.pdf");
		File [] fileList = folder.listFiles(mFF);
		for (int i = 0; i < fileList.length; i++) {
			File in = fileList[i];
			File out = new File("Users/marialyven/Downloads/76_Sample_Chapter.pdf");
		try {
			CopyFile.fileCopy(in, out);
		} catch (IOException e) {
			e.printStackTrace();
			
		}
			
		}
		

	}

}
