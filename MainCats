package com.gmail.marialyven12;

public class Main {

	public static void main(String[] args) {
		Cat catOne = new Cat();
		catOne.setColor("Yellow");
		catOne.setName("Petrusha");
		catOne.setWeight(10.2);
		catOne.setAge(14);
		catOne.setBreed("British");
		System.out.println(catOne);
		System.out.println();
	
		Cat catTwo = new Cat();
		catTwo.setColor("Grey");
		catTwo.setName("Kosya");
		catTwo.setWeight(6.5);
		catTwo.setAge(1);
		catTwo.setBreed("Dvorovoy");
		System.out.println(catTwo);
		System.out.println();
	
	}
	}
	
